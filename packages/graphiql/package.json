{
  "name": "graphiql",
  "version": "2.1.0",
  "description": "An graphical interactive in-browser GraphQL IDE.",
  "contributors": [
    "Hyohyeon Jeong <asiandrummer@fb.com>",
    "Lee Byron <lee@leebyron.com> (http://leebyron.com/)"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/graphql/graphiql",
    "directory": "packages/graphiql"
  },
  "homepage": "http://github.com/graphql/graphiql/tree/master/packages/graphiql#readme",
  "bugs": {
    "url": "https://github.com/graphql/graphiql/issues?q=issue+label:graphiql"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "esm",
    "src",
    "graphiql.js",
    "graphiql.min.js",
    "graphiql.min.js.map",
    "graphiql.css",
    "graphiql.min.css",
    "graphiql.min.css.map"
  ],
  "scripts": {
    "build-bundles": "yarn build:vite:bundles && yarn build:vite:html",
    "build": "yarn build:clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc",
    "build:clean": "rimraf esm dist",
    "build:esm": "tsc --project ./tsconfig.esm.json",
    "build:vite:bundles": "vite build --config ./vite/vite.build.bundle.config.ts && MIN=1 vite build --config ./vite/vite.build.bundle.config.ts",
    "build:vite:html": "vite build --config ./vite/vite.build.html.config.ts",
    "bundles:clean": "rimraf build",
    "check": "tsc --noEmit",
    "clean": "yarn build:clean && yarn bundles:clean",
    "dev": "concurrently 'yarn start:e2e:server' 'yarn vite build --watch --mode GRAPHIQL_DEV' 'vite --mode GRAPHIQL_DEV'",
    "e2e": "yarn build-bundles && start-test 'yarn run start:e2e:server' 8080 'vite --mode GRAPHIQL_E2E' 'http-get://127.0.0.1:5174/?query={test { id }}' 'cypress run'",
    "start:e2e:server": "PORT=8080 node test/e2e-server"
  },
  "dependencies": {
    "@graphiql/react": "^0.14.0",
    "@graphiql/toolkit": "^0.8.0",
    "entities": "^2.0.0",
    "graphql-language-service": "^5.1.0",
    "markdown-it": "^12.2.0"
  },
  "peerDependencies": {
    "graphql": "^15.5.0 || ^16.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@types/codemirror": "^5.60.5",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^14.14.22",
    "@types/testing-library__jest-dom": "5.14.5",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.8.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.1",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "express-graphql": "experimental-stream-defer",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "graphql": "^16.4.0",
    "graphql-subscriptions": "^2.0.0",
    "html-webpack-plugin": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^16.13.1",
    "require-context.macro": "^1.2.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "router": "^1.3.7",
    "serve": "^11.3.0",
    "start-server-and-test": "^1.10.11",
    "style-loader": "^1.1.3",
    "subscriptions-transport-ws": "0.11.0",
    "ts-loader": "^7.0.0",
    "typescript": "^4.6.3",
    "vite": "3.2.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-require-transform": "^1.0.4",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "ws": "^8.3.0"
  }
}
